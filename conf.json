{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.(vue|js)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "recurse": true,
        "destination": "doc"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-vue"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true
        },
        "applicationName": "WEB APP 框架",
        "openGraph": {},
        "meta": {}
    }
}
