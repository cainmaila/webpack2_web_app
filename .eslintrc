{
	"parserOptions": {
   		"ecmaVersion": 8,
   		"sourceType": "module",
	},
	"rules": {
		"indent": ["error", 4],
		"no-console":"warn",
		"eqeqeq":"warn",
		"no-alert": "warn",
		"no-eval":"error",
		"no-undef-init":"error",
		"no-unused-vars":"warn",
		"no-var": "error",
		"no-const-assign":"error",
		"no-dupe-class-members": "error",
		"no-extra-semi": "error",
		"camelcase":"error",
		"object-curly-newline":["warn", {
			"ObjectExpression": "always",
			"ObjectPattern": {"multiline": true}
		}]
	}	
}